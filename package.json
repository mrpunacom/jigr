{
  "name": "jigr-app",
  "version": "1.11.2.001",
  "private": true,
  "description": "JiGR - Hospitality Compliance Platform for New Zealand businesses",
  "repository": {
    "type": "git",
    "url": "https://github.com/mrpunacom/jigr.git"
  },
  "homepage": "https://github.com/mrpunacom/jigr#readme",
  "bugs": {
    "url": "https://github.com/mrpunacom/jigr/issues"
  },
  "author": "JiGR Team",
  "browserslist": [
    "iOS >= 12",
    "Safari >= 12"
  ],
  "scripts": {
    "dev": "npm run increment-build && next dev",
    "build": "next build",
    "build-with-scripts": "npm run increment-build && npm run generate-static-version && next build",
    "start": "next start",
    "lint": "next lint",
    "db:generate-types": "supabase gen types typescript --local > types/supabase.ts",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:reset": "supabase db reset",
    "increment-build": "node \":assets/scripts/increment-build.js\"",
    "log": "node \":assets/scripts/log-change.js\"",
    "alpha": "node \":assets/scripts/increment-alpha.js\"",
    "production": "node \":assets/scripts/mark-production.js\"",
    "version-check": "cat version.json",
    "generate-static-version": "node \":assets/scripts/generate-static-version.js\"",
    "archive": "node \":assets/scripts/archive-manager.js\"",
    "cleanup": "node \":assets/scripts/cleanup-demo-data.js\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:api": "jest --testPathPattern=api",
    "test:integration": "jest --testPathPattern=integration",
    "test:unit": "jest --testPathPattern=unit",
    "test:e2e": "jest --testPathPattern=e2e",
    "test:barcode": "jest --testPathPattern=barcode",
    "test:stock": "jest --testPathPattern=stock",
    "test:recipes": "jest --testPathPattern=recipes",
    "test:menu": "jest --testPathPattern=menu"
  },
  "dependencies": {
    "@ericblade/quagga2": "^1.8.4",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.38.0",
    "@tabler/icons-react": "^3.35.0",
    "@types/web-bluetooth": "^0.0.21",
    "@zxing/library": "^0.21.3",
    "date-fns": "^2.30.0",
    "dotenv": "^17.2.1",
    "form-data": "^4.0.4",
    "google-auth-library": "^8.9.0",
    "googleapis": "^166.0.0",
    "jsbarcode": "^3.12.1",
    "next": "^15.4.5",
    "node-fetch": "^3.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.3.8",
    "resend": "^6.1.0",
    "stripe": "^14.0.0"
  },
  "devDependencies": {
    "@iconify-json/tabler": "^1.2.23",
    "@iconify/tailwind": "^1.2.0",
    "@jest/globals": "^30.1.2",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^18.19.50",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/supertest": "^6.0.2",
    "autoprefixer": "^10.4.20",
    "babel-jest": "^29.7.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "^12.3.4",
    "flyonui": "^2.4.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-html-reporters": "^3.1.6",
    "postcss": "^8.4.47",
    "supabase": "^1.100.0",
    "supertest": "^6.3.3",
    "tailwindcss": "^3.4.13",
    "typescript": "^4.9.5"
  }
}
